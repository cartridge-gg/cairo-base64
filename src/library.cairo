from starkware.cairo.common.math import unsigned_div_rem
from starkware.cairo.common.bitwise import bitwise_and, bitwise_or
from starkware.cairo.common.cairo_builtins import BitwiseBuiltin
from starkware.cairo.common.registers import get_label_location
from starkware.cairo.common.math_cmp import is_le

func encode3{range_check_ptr, bitwise_ptr: BitwiseBuiltin*}(value: felt, lookup: felt*) -> (c0: felt, c1: felt, c2: felt, c3: felt, r: felt):
    alloc_locals

    let (q00, a0) = unsigned_div_rem(value, 256)
    let (q01, a1) = unsigned_div_rem(q00, 256)
    let (q02, a2) = unsigned_div_rem(q01, 256)

    let (_, r00) = unsigned_div_rem(a2, 2 ** 16)
    let r01 = r00 * 2 ** 16

    let (_, r10) = unsigned_div_rem(a1, 2 ** 24)
    let r11 = r10 * 2 ** 8

    let (n0) = bitwise_or(r01, r11)
    let (n) = bitwise_or(n0, a0)

    let (c00, _) = unsigned_div_rem(n, 2 ** 18)
    let (c01) = bitwise_and(c00, 63)

    let (c10, _) = unsigned_div_rem(n, 2 ** 12)
    let (c11) = bitwise_and(c10, 63)

    let (c20, _) = unsigned_div_rem(n, 2 ** 6)
    let (c21) = bitwise_and(c20, 63)

    let (c30) = bitwise_and(n, 63)

    [ap] = c01; ap++
    call abs lookup
    let c0 = [ap - 1]

    [ap] = c11; ap++
    call abs lookup
    let c1 = [ap - 1]

    [ap] = c21; ap++
    call abs lookup
    let c2 = [ap - 1]

    [ap] = c30; ap++
    call abs lookup
    let c3 = [ap - 1]

    return (c0=c0, c1=c1, c2=c2, c3=c3, r=q02)
end

func _encode3_inner{range_check_ptr, bitwise_ptr: BitwiseBuiltin*}(value: felt, padding: felt, encoded: felt, round: felt, lookup: felt*) -> (encoded: felt):
    alloc_locals

    if value == 0:
        return (encoded)
    end

    let (c0, c1, c2, c3, r) = encode3(value, lookup)
    let o = round * 32
    let (o0) = pow2(o)
    let (o1) = pow2(8 + o)
    let (o2) = pow2(16 + o)
    let (o3) = pow2(24 + o)

    if padding == 1:
        return _encode3_inner(
            r, 
            0, 
            encoded + c0 * o3 + c1 * o2 + c2 * o1 + '=' * o0, 
            round + 1,
            lookup
        )
    end

    if padding == 2:            
        return _encode3_inner(
            r, 
            0, 
            encoded + c0 * o3 + c1 * o2 + '=' * o1 + '=' * o0, 
            round + 1,
            lookup
        )
    end

    return _encode3_inner(
            r, 
            0, 
            encoded + c0 * o3 + c1 * o2 + c2 * o1 + c3 * o0, 
            round + 1,
            lookup
        )
end

func calc_offset_padding{range_check_ptr}(n: felt) -> (offset: felt, padding: felt):
    let (le0) = is_le(n, 255)
    if le0 == 1:
        return (65536, 2)
    end

    let (le1) = is_le(n, 65536)
    if le1 == 1:
        return (256, 1)
    end

    let (le2) = is_le(n, 16777216)
    if le2 == 1:
        return (1, 0)
    end

    let (truncated, _) = unsigned_div_rem(n, 2 ** 24)
    return calc_offset_padding(truncated)
end

